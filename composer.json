{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "amphp/websocket-client": "^2.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "cboden/ratchet": "^0.4.4",
        "dgvai/laravel-adminlte-components": "^2.1",
        "doctrine/dbal": "^3.7",
        "elibyy/tcpdf-laravel": "^10.0",
        "ellgreen/laravel-loadfile": "^3.0",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.8",
        "hellosign/hellosign-php-sdk": "3.*@dev",
        "ilovepdf/ilovepdf-php": "*",
        "inspector-apm/inspector-laravel": "^4.7",
        "intervention/image": "^2.7",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "maatwebsite/excel": "*",
        "phpoffice/phpspreadsheet": "^1.18",
        "protonemedia/laravel-splade": "^1.4",
        "psr/simple-cache": "2.0",
        "pusher/pusher-php-server": "^7.2",
        "ratchet/pawl": "^0.4.1",
        "select2/select2": "^4.0",
        "spatie/laravel-webhook-client": "^3.2",
        "spatie/simple-excel": "^3.3",
        "yajra/laravel-datatables": "^9.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.8",
        "laravel/sail": "^1.0.1",
        "laravel/ui": "^4.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "classmap-authoritative": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
